* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
*::after {
  box-sizing: border-box;
}
*::before {
  box-sizing: border-box;
}
a {
  color: inherit;
  text-decoration: none;
}
body {
  background: #eee;
}
.globalHeader {
  margin: 20px;
}
.searchForm {
  border: 1px solid red;
  display: flex;
}
.searchForm > input {
  width: 100%;
}
